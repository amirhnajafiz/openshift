apiVersion: v1
kind: Service
metadata:
  name: hubble-ui
  namespace: cilium
spec:
  type: LoadBalancer
  ports:
    - port: 80
      targetPort: 12000
      protocol: TCP
      name: http
    - port: 443
      targetPort: 12000
      protocol: TCP
      name: https
  selector:
    k8s-app: hubble-ui
